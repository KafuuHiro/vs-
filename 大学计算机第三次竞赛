#include <iostream>
#include<time.h>
#include<math.h>
int a[5];

int main()
{
    int i;
    for (i = 0; i < 5; i++)
    {
        a[i] = rand() % 101;
        printf("%d\n", a[i]);
    }
    int min = abs(a[0] - 50);
    for (i = 0; i < 5; i++)
    {
        if (min >= abs(a[i] - 50))
        {
            min = abs(a[i] - 50);
        }
        else {}
    }
    for (i = 0; i < 5; i++)
    {
        if (min == abs(a[i] - 50))
        {
            printf("result is %d", a[i]);
            break;
        }
        else {}
    }
    return 0;
}
